{
  "$schema": "https://json.schemastore.org/prettierrc",
  "plugins": [
    "prettier-plugin-organize-attributes",
    "prettier-plugin-organize-imports",
    "prettier-plugin-tailwindcss"],
  "semi": true,
  "singleQuote": false,
  "useTabs": false,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "printWidth": 120,
  "trailingComma": "all"
}
